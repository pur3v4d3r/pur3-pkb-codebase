"editor.tokenColorCustomizations": {
  "comments": "#808080",
  "strings": "#E74C3C",
  "keywords": "#C0392B",
  "functions": "#E74C3C",
  "variables": "#A0A0A0",
  "numbers": "#EC7063",
  "types": "#B22222",
  "textMateRules": [
      {
          "scope": [
              "keyword.control",
              "keyword.operator.new",
              "keyword.other.important"
          ],
          "settings": {
              "foreground": "#C0392B",
              "fontStyle": "bold"
          }
      },
      {
          "scope": [
              "entity.name.function",
              "support.function"
          ],
          "settings": {
              "foreground": "#E74C3C"
          }
      },
      {
          "scope": [
              "variable",
              "variable.other",
              "variable.parameter"
          ],
          "settings": {
              "foreground": "#A0A0A0"
          }
      },
      {
          "scope": [
              "entity.name.type",
              "entity.name.class",
              "support.type",
              "support.class"
          ],
          "settings": {
              "foreground": "#B22222",
              "fontStyle": "italic"
          }
      },
      {
          "scope": [
              "string",
              "string.quoted"
          ],
          "settings": {
              "foreground": "#E74C3C"
          }
      },
      {
          "scope": [
              "constant.numeric",
              "constant.language"
          ],
          "settings": {
              "foreground": "#EC7063"
          }
      },
      {
          "scope": [
              "comment",
              "punctuation.definition.comment"
          ],
          "settings": {
              "foreground": "#808080",
              "fontStyle": "italic"
          }
      },
      {
          "scope": [
