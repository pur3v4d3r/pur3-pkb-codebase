      {
          "scope": "punctuation.definition.markdown",
          "settings": {
              "foreground": "#C0392B"
          }
      },
      {
          "scope": [
              "beginning.punctuation.definition.list.markdown",
              "punctuation.definition.list.begin.markdown"
          ],
          "settings": {
              "foreground": "#E74C3C"
          }
      },
      {
          "scope": "markup.strikethrough.markdown",
          "settings": {
              "foreground": "#808080",
              "fontStyle": "strikethrough"
          }
      }
  ]
},
