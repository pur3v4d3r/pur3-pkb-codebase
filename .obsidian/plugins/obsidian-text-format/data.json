        {
          "search": "`",
          "replace": ""
        }
      ]
    },
    {
      "id": "494950b7-7ba1-4340-a518-3b29734cd438",
      "name": "Remove Bold Formatting",
      "data": [
        {
          "search": "\\*\\*(.+?)\\*\\*",
          "replace": "$1"
        }
      ]
    },
    {
      "id": "a74e86fc-e490-4619-95cd-22fe16ed1a6b",
      "name": "Remove italic formatting",
      "data": [
        {
          "search": "\\*(.+?)\\*",
          "replace": "$1"
        }
      ]
    },
    {
      "id": "632b937d-c13b-4747-ab31-e7170b9135a6",
      "name": "Remove markdown links keep text",
      "data": [
        {
          "search": "\\[([^\\]]+)\\]\\([^\\)]+\\)",
          "replace": "$1"
        }
      ]
    },
    {
      "id": "41a0f393-9187-4744-9270-33a7c88af288",
      "name": "Remove checkboxes",
      "data": [
        {
          "search": " - \\[ \\]",
          "replace": "-"
        }
      ]
    },
    {
      "id": "86dcfa72-c911-47ca-a616-eb174382cd38",
      "name": "Check all tasks",
      "data": [
        {
          "search": "- \\[ \\]",
          "replace": "- [x]"
        }
      ]
    },
    {
      "id": "c850e2d5-89a6-486c-97c1-510edfce3f89",
      "name": "Uncheck all tasks",
      "data": [
        {
          "search": "- \\[x\\]",
          "replace": "- [ ]"
        }
      ]
    },
    {
      "id": "a8b68b27-5199-4258-8331-7776511d447c",
      "name": "Remove task markers",
      "data": [
        {
          "search": "- \\[[x ]\\]",
