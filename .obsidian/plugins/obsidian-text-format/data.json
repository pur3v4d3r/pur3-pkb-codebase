  "customReplaceList": [
    {
      "id": "remove-trailing-spaces",
      "name": "Remove trailing spaces",
      "data": [
        {
          "search": "(\\s*)(?=\\n)|(\\s*)$",
          "replace": ""
        }
      ]
    },
    {
      "id": "remove-blank-line",
      "name": "Remove blank line(s)",
      "data": [
        {
          "search": "\\n\\s*\\n",
          "replace": "\\n"
        }
      ]
    },
    {
      "id": "add-line-break",
      "name": "Add extra line break between paragraphs",
      "data": [
        {
          "search": "\\n",
          "replace": "\\n\\n"
        }
      ]
    },
    {
      "id": "split-lines-by-blank",
      "name": "Split line(s) by blanks",
      "data": [
        {
          "search": " ",
          "replace": "\\n"
        }
      ]
    },
    {
      "id": "9e4e73b0-7ae9-4824-a7ad-946b5837d7e5",
      "name": "Remove -",
