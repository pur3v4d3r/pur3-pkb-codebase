{
  "schemaVersion": "0.1.0",
  "name": "Pur3v4d3r Quick Capture",
  "behavior": "create",
  "noteNameFormat": "{{date|date:\"YYYY-MM-DD\"}}-clip-{{title|safe_name|slice:0,40}}",
  "path": "00-inbox/webclips",
  "context": "{{content|slice:0,6000}}",
  "properties": [
    {
      "name": "aliases",
